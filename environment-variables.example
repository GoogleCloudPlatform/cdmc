echo -e "\t\tPROJECT_NUMBER         :" $PROJECT_NUMBER
echo -e "\t\tPROJECTI_ID_GOV        :" $PROJECT_ID_GOV
echo -e "\t\tREGION                 :" $REGION
echo -e "\t\tGCS_BUCKET_TPCDI       :" $GCS_BUCKET_TPCDI
