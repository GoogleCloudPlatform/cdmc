# Set the environment variables

# The project where the data will be loaded, and the controls will run
export PROJECT_ID="UPDATE WITH YOUR VALUE" 

# The governance project, used for 
export PROJECT_ID_GOV="UPDATE WITH YOUR VALUE" 
export REGION="us-central1"
export PROJECT_NUMBER=$(gcloud projects describe $PROJECT_ID --format="value(projectNumber)")
export GCS_BUCKET_TPCDI=${PROJECT_NUMBER}-tpcdi-data
#export KMS_KEY='projects/sdw-data-gov-b1927e-dd69/locations/us-central1/keyRings/cmek-keyring-bigquery/cryptoKeys/cmek-bigquery-hsm-key'  # replace with your key name

echo -e "\nYour environment variables have been initialised as follows:"
echo -e "\tModify anything that does not look right:"
